{
  "name": "watson-html5-speech-recognition",
  "version": "0.6.0",
  "description": "Watson HTML5 Speech to Text",
  "main": "index.js",
  "scripts": {
    "compile": "./node_modules/.bin/babel ./lib -o index.js",
    "compile:browserify": "./node_modules/.bin/browserify ./lib/index.js -o index.js -t [ babelify --presets [ es2015 stage-0 ] ]",
    "compile:example": "rm -rf example/server/dist && mkdir example/server/dist && ./node_modules/.bin/browserify example/server/public/main.js -o ./example/server/dist/main.js",
    "compile:example2": "rm -rf example/server/dist && mkdir example/server/dist && ./node_modules/.bin/browserify example/server/public/main.js -o ./example/server/dist/speech.js  -t [ babelify --presets [ es2015 stage-0 ] ]",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "watson",
    "speech-to-text",
    "speech",
    "text",
    "voice",
    "recognition"
  ],
  "author": "Carmine DiMascio <cdimascio@gmail.com> (https://github.com/cdimascio)",
  "license": "Apache-2.0",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "watson-speech": "^0.17.1"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.3.0"
  }
}
